  ENDDIGIT -> END;
  HEXDIGIT -> END;
  OCTDIGIT -> END;
  BINDIGIT -> END;
  SECONDS -> END;
  DAY -> END;
  Z -> END;
  ZMINUTES -> END;
  0 -> END;
  NUMNULL -> END;
  DIGIT -> END [label = "Integer"];
