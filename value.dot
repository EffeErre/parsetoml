      GENDDIGIT -> ZONESEP;
      GSECONDS -> ZONESEP
      GENDDIGIT -> Z;
      GENDDIGIT -> GENDDIGIT;
      GSECONDS -> Z;
      ZONESEP -> ZHOURS -> ZHMSEP -> ZMINUTES;
      label = "DateTime";
    }
  }
  SUBSEP -> ENDDIGIT;
  "." -> ENDDIGIT;
  DIGIT -> ".";
  NUMNULL -> ".";
  subgraph cluster_floats {
    "." -> MOREDIGITS;
    MOREDIGITS -> E
